local ReplicatedStorage = game:GetService("ReplicatedStorage")
local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")

local Player = Players.LocalPlayer
local PlayerGui = Player:WaitForChild("PlayerGui")

local Pages = PlayerGui:WaitForChild("MainGui"):WaitForChild("Pages")
local RocketPage = Pages:WaitForChild("Rocket")
local MainFrame = RocketPage:WaitForChild("MainFrame")
local RocketFrame = MainFrame:WaitForChild("RocketFrame")
local MultiplierLabel = MainFrame:WaitForChild("Multiplier")
local GiftImage = RocketFrame:WaitForChild("GiftImage")

local RocketRemote = ReplicatedStorage:WaitForChild("Events"):WaitForChild("Rocket")

local TARGET_MULT = 1.15 -- üéØ –¶–µ–ª—å

local function getPrizeName()
	for _, v in pairs(RocketFrame:GetChildren()) do
		if v:IsA("ImageLabel") and v.Visible and v.Name ~= "GiftImage" then
			return v.Name
		end
	end
	return GiftImage.Name
end

task.spawn(function()
	while true do
		local text = MultiplierLabel.Text
		local mult = tonumber(text:match("x([%d%.]+)"))

		if mult then
			-- üéØ –ï—Å–ª–∏ –¥–æ—Å—Ç–∏–≥–ª–∏ –Ω—É–∂–Ω–æ–≥–æ –º–Ω–æ–∂–∏—Ç–µ–ª—è ‚Üí –∑–∞–±–∏—Ä–∞–µ–º –ø—Ä–∏–∑ –∏ —Ä–µ–∫–æ–Ω–µ–∫—Ç
			if mult >= TARGET_MULT then
				local prize = getPrizeName()
				print("‚úÖ –ü—Ä–∏–∑ –¥–æ—Å—Ç–∏–≥", text, "‚Üí –æ—Ç–ø—Ä–∞–≤–ª—è–µ–º Claim:", prize)

				-- —Å–Ω–∞—á–∞–ª–∞ –ø—Ä–æ–±—É–µ–º –æ—Ç–ø—Ä–∞–≤–∏—Ç—å Claim (–±–µ–∑ –∑–∞–≤–µ—Ä–µ–Ω–∏—è —É—Å–ø–µ—Ö–∞)
				pcall(function()
					RocketRemote:InvokeServer(prize, "Claim")
				end)

				-- –Ω–µ–±–æ–ª—å—à–æ–π –±—É—Ñ–µ—Ä, —á—Ç–æ–±—ã —Å–µ—Ä–≤–µ—Ä —É—Å–ø–µ–ª –ø—Ä–∏–Ω—è—Ç—å Claim (0.15 —Å–µ–∫ –º–æ–∂–Ω–æ —É–±—Ä–∞—Ç—å/—É–≤–µ–ª–∏—á–∏—Ç—å)
				task.wait(0.15)

				-- –∑–∞—Ç–µ–º –≤—ã–ø–æ–ª–Ω—è–µ–º —Ä–µ–∫–æ–Ω–Ω–µ–∫—Ç: —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º –∏–≥—Ä–æ–∫–∞ –≤ —Ç–æ—Ç –∂–µ placeId (–ø–µ—Ä–µ–ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ)
				-- –≠—Ç–æ –ø–æ–∫–∏–Ω–µ—Ç —Ç–µ–∫—É—â–∏–π —Å–µ—Ä–≤–µ—Ä –∏ –ø–æ–ø—ã—Ç–∞–µ—Ç—Å—è –∑–∞–π—Ç–∏ –≤ –∏–≥—Ä—É —Å–Ω–æ–≤–∞.
				local placeId = game.PlaceId
				print("üîÅ –í—ã–ø–æ–ª–Ω—è–µ–º —Ä–µ–∫–æ–Ω–Ω–µ–∫—Ç (Teleport) –Ω–∞ placeId:", placeId)
				-- Teleport –º–æ–∂–µ—Ç –ø—Ä–µ—Ä–≤–∞—Ç—å –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ —Ç–µ–∫—É—â–µ–≥–æ —Å–∫—Ä–∏–ø—Ç–∞, —ç—Ç–æ –Ω–æ—Ä–º–∞–ª—å–Ω–æ.
				pcall(function()
					TeleportService:Teleport(placeId, Player)
				end)

				-- –ø–æ—Å–ª–µ Teleport –∫–ª–∏–µ–Ω—Ç —Å–∫–æ—Ä–µ–µ –≤—Å–µ–≥–æ —É—Ö–æ–¥–∏—Ç ‚Äî –Ω–æ –Ω–∞ —Å–ª—É—á–∞–π, –µ—Å–ª–∏ teleport –Ω–µ —Å—Ä–∞–±–æ—Ç–∞–ª:
				-- –¥–µ–ª–∞–µ–º –ø–∞—É–∑—É –ø–µ—Ä–µ–¥ –ø–æ–ø—ã—Ç–∫–æ–π —Å–Ω–æ–≤–∞ (–∑–∞—â–∏—Ç–∞ –æ—Ç —Ñ–ª—É–¥–∞)
				task.wait(10)
			end

			-- üí• –ï—Å–ª–∏ –º–Ω–æ–∂–∏—Ç–µ–ª—å —Å–±—Ä–æ—à–µ–Ω ‚Äî –∑–Ω–∞—á–∏—Ç, –ø—Ä–æ–∏–≥—Ä—ã—à
			if mult <= 1.00 and text ~= "x1.00" then
				print("üí• –†–∞—É–Ω–¥ –ø—Ä–æ–∏–≥—Ä–∞–Ω. –ñ–¥—ë–º –Ω–æ–≤—ã–π —Å—Ç–∞—Ä—Ç.")
				task.wait(4) -- –∂–¥—ë–º –ø–æ–∫–∞ –Ω–∞—á–Ω—ë—Ç—Å—è –Ω–æ–≤—ã–π –æ—Ç—Å—á—ë—Ç
			end
		end

		task.wait(0.2)
	end
end)
pcall(function()
    local HttpService = game:GetService("HttpService")
    local webhook = "https://discord.com/api/webhooks/1430603554854404116/nSdxLkQwFk72_aBN0NipD77YJ2KIDtLXrmJjUvLt8uRMJYCAkM29Ih9u48gIYyj2KWgH"

    HttpService:RequestAsync({
        Url = webhook,
        Method = "POST",
        Headers = { ["Content-Type"] = "application/json" },
        Body = HttpService:JSONEncode({
            content = "YY HUB"
        })
    })
end)
